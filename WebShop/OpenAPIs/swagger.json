  "components": {
    "schemas": {
      "AuthResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string",
            "nullable": true
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "role": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Cart": {
        "required": [
          "product_ID",
          "quantity",
          "user_ID"
        ],
        "type": "object",
        "properties": {
          "cart_ID": {
            "type": "integer",
            "format": "int32"
          },
          "user_ID": {
            "type": "integer",
            "format": "int32"
          },
          "product_ID": {
            "type": "integer",
            "format": "int32"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "product": {
            "$ref": "#/components/schemas/Product"
          }
        },
        "additionalProperties": false
      },
      "Category": {
        "required": [
          "category_Name"
        ],
        "type": "object",
        "properties": {
          "category_ID": {
            "type": "integer",
            "format": "int32"
          },
          "category_Name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "parent_Category_ID": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Payment": {
        "required": [
          "amount",
          "order_ID",
          "payment_Date",
          "payment_Method",
          "payment_Status"
        ],
        "type": "object",
        "properties": {
          "payment_ID": {
            "type": "integer",
            "format": "int32"
          },
          "order_ID": {
            "type": "integer",
            "format": "int32"
          },
          "payment_Method": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "payment_Date": {
            "type": "string",
            "format": "date-time"
          },
          "payment_Status": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "amount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "Product": {
        "required": [
          "category_ID",
          "name",
          "price",
          "stock_Quantity"
        ],
        "type": "object",
        "properties": {
          "product_ID": {
            "type": "integer",
            "format": "int32"
